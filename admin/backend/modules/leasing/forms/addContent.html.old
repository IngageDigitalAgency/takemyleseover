<code>
$("#popup select").chosen();
$("#tabs" ).tabs();
$('.def_field_datepicker').datepicker({
	changeMonth: true,
	changeYear:true
});
$('.def_field_datetimepicker').datepicker({
	changeMonth: true,
	changeYear:true
});
$('.popovers').popover();
sortableOptions();
$('#owner_id_chosen .chosen-search input').autocomplete({
	source:function(request,response) {
		$.ajax({
			url:'/modit/ajax/getNames/leasing',
			data:{'s':request.term,'m':$("input[name=p_id]").val()},
			context:this,
			success:function(list,status,xhr) {
				try {
					obj = eval("("+list+")");
					$("select[name=owner_id]").html(obj.html).trigger("chosen:updated");
					if (obj.messages.length > 0) showPopupError(obj.messages);
				}
				catch(err) {
					showPopupError(err.message);
				}
			}
		});
	}
});

</code>
