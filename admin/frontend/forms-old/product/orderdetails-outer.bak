<div class="receipt">
	<div class="clearfix">
		<h3 class="pull-left">Your Receipt</h3>
		<h3 class="pull-right">Order #: %%id%%</h3>
	</div>
</div>
<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th class="col-md-1 text-center">Quantity</th>
			<th class="col-md-3 text-left">Product</th>
			<th class="col-md-2 text-right">Price</th>
			<th class="col-md-2 text-right">Extended</th>
		</tr>
	</thead>
	<tbody>
		%%lines%%
	</tbody>
</table>
%%custom_note%%
<div class="row">
<!if(strlen("%%custom_image_1%%")>0)!>
	<div class="col-sm-3">
		<div class="single-project-item">
			<div class="img-box">
				<img class="def_field_image" src="%%custom_image_1%%" />
				<div class="overlay">
					<div class="box">
						<div class="content">
							<div class="top">
								<ul class="list-inline">
									<li>
										<a data-group="1" href="%%custom_image_1%%" class="img-popup"><i class="fa fa-search-plus"></i></a>
									</li>
								</ul><!-- /.list-inline -->
							</div><!-- /.top -->
						</div><!-- /.content -->
					</div><!-- /.box -->
