<section class="section-top" id="accordion-section">
	<div class="container">
		<div class="row">
			<div class="col-xs-12">
				<div class="section-header-2">
					<h4>%%title%%</h4>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12">
				<div class="panel-group" id="accordion">
					%%articles%%
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-xs-12 text-center">%%pagination%%</div>
		</div>
	</div>
</section>
<script type="text/javascript">
pagination = function(pg,el) {
$.ajax({
	url: "/ajax/render",
	data: {t_id:%%module:fetemplate_id%%,pagenum:pg,folder_id:%%module:folder_id%%},
	success: function(obj, xhr, status) {
		try {
			h = eval("("+obj+")");
			d = $("<div/>");
			$(d).html(h.html);
			$("#accordion-section").replaceWith(h.html);
		}
		catch(err) {
		}
	}
});
}
</script>
