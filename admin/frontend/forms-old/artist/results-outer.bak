<div id="search-results" class="top20 bottom20">
	<div class="row">
		<div class="col-xs-14 col-xs-offset-1">
			<div class="title text-center">Your Search Results [%%count%%]</div>
			<div class="results">
<!if((int)"%%count%%"==0)!>
				<div class="row">
					<div class="col-xs-8 col-xs-offset-4">
						<div class="alert alert-warning text-center">Your search did not return any results</div>
					</div>
				</div>
<!else!>
				%%artists%%
				<form method="POST" action="/search-results" >
					<div class="hidden">%%search%%%%artistSearch%%%%pagenum%%</div>
					<div class="row">
						<div class="col-xs-16 text-center">
							%%pagination,false%%
						</div>
					</div>
				</form>
<!fi!>
			</div>
		</div>
	</div>
<!fi!>
</div>
<script type="text/javascript">
pagination = function(pg) {
	f = $("#search-results form")[0];
	$(f).find("input[name='pagenum']").val(pg);
	$(f).submit();
}
</script>