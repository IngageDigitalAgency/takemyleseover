<article class="news-details">
	<div class="row">
		<div class="col-xs-16">
			<div class="images">
<!if(strlen("%%video:video_id%%") > 0)!>
				<div id="blog-post-video">
					<div class="embed-responsive embed-responsive-16by9">
						<iframe src="http://www.youtube.com/embed/%%video:video_id%%" class="embed-responsive-item"></iframe>
					</div>
				</div>
<!else!>
				<div class="p-img-scale-width" >%%img_image1%%</div>
<!fi!>
			</div>
			<h1>%%title%%</h1>
			<h2>%%alternate_title%%</h2>
			<div class="tinymce">%%body%%</div>
		</div>
	</div>
</article>